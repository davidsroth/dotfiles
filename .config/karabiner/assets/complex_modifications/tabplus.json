{
  "title": "Tab plus",
  "rules": [
    {
      "description": "Tab if alone, else fn",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "fn",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "tab"
            }
          ]
        }
      ]
    }
  ]
}
